{
    "name": "Penguini Steam Enhancement",
    "version": "1.0.0",
    "description": "Adds extended functionality and cosmetic elements to Steam.",
    "manifest_version": 2,
    "content_scripts": [
        {
            "js": ["listings/listings.js"],
            "matches": ["https://steamcommunity.com/market/listings/*"]
        }
    ],
    "permissions": [],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}